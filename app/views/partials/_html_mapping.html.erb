<% @newHash.each do |key, value| %>
	<% if key == params[:proverb] %>
		The word <b><%= key %></b> occurs <%= value %> times in Proverbs <%= @common.first.chapter_num %>.<br>
		<% key_values.each do |key, value| %>
			<% if key == params[:proverb] %>
				The word <b><%= key %></b> occurs <%= value %> times in the book of Proverbs.<br><br>
				<% begin %>
					<% entry = client.entry(params[:proverb]) %>
					<% entries = entry.lexical_entries[0].entries %>
					<% @meanings =  entries[0][:senses][0][:definitions] %>
					<% if @meanings.length == 0 %>
						No Oxford Dictionary entry available for the word <%= params[:proverb] %>.
					<% else %>
						<% if @meanings.length == 1 %>
							Definition of <%= params[:proverb] %>:<br>
						<% else %>
							Definitions of <%= params[:proverb] %>:<br>
						<% end %>
						<% @meanings.each do |m| %>
							â€¢ <%= m %><br>
						<% end %>
					<% end %>

				<% rescue OxfordDictionary::Error %>
					No Oxford Dictionary entry available for the word <%= params[:proverb] %>.<br>
				<% end %>
				<br>
				Box and Whisker Plot breakdown of recurring words:<br>
				Least: <%= bwp.least %><br>
				First Quartile: <%= bwp.first_quartile %><br>
				Median: <%= bwp.median %><br>
				Third Quartile: <%= bwp.third_quartile %><br>
				Greatest: <%= bwp.greatest %><br>

			<% end %>
		<% end %>
	<% end %>
<% end %>


